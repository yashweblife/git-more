{
  "name": "git-more",
  "displayName": "Git More",
  "description": "Get more shortcuts for git",
  "publisher": "yashweblife",
  "version": "0.1.5",
  "repository": "https://github.com/yashweblife/git-more",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "activationEvents": [
    "git-more.stage",
    "git-more.commit",
    "git-more.push",
    "git-more.pull",
    "git-more.view",
    "git-more.checkout",
    "git-more.stageSelector"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-more.stage",
        "title": "Git More: Stage File"
      },
      {
        "command": "git-more.commit",
        "title": "Git More: Commit Staged Files"
      },
      {
        "command": "git-more.push",
        "title": "Git More: Git Push Changes"
      },
      {
        "command": "git-more.pull",
        "title": "Git More: Git Pull"
      },
      {
        "command": "git-more.view",
        "title": "Git More: Visualizer"
      },
      {
        "command": "git-more.checkout",
        "title": "Git More: Checkout"
      },
      {
        "command": "git-more.merge",
        "title": "Git More: Merge"
      },
      {
        "command": "git-more.newbranch",
        "title": "Git More: New Branch"
      },
      {
        "command": "git-more.fetcher",
        "title": "Git More: Fetch"
      },
      {
        "command": "git-more.syncer",
        "title": "Git More: Sync"
      },
      {
        "command": "git-more.stageSelector",
        "title": "Git More: Stage Select"
      }
    ],
    "keybindings":[
      {
        "win": "ctrl+shift+win+left",
        "mac": "ctrl+cmd+left",
        "command": "git-more.stage",
        "key": "ctrl+meta+left"
      },
      {
        "win": "ctrl+shift+win+right",
        "mac": "ctrl+cmd+right",
        "command": "git-more.commit",
        "key": "ctrl+meta+right"
      },
      {
        "win": "ctrl+shift+win+up",
        "mac": "ctrl+cmd+up",
        "command": "git-more.push",
        "key": "ctrl+meta+up"
      },
      {
        "win": "ctrl+shift+win+down",
        "mac": "ctrl+cmd+down",
        "command": "git-more.pull",
        "key": "ctrl+meta+down"
      },
      {
        "win": "ctrl+shift+win+v",
        "mac": "ctrl+cmd+v",
        "command": "git-more.view",
        "key": "ctrl+meta+v"
      },
      {
        "win": "ctrl+shift+win+c",
        "mac": "ctrl+cmd+c",
        "command": "git-more.checkout",
        "key": "ctrl+meta+c"
      },
      {
        "win": "ctrl+shift+win+left",
        "mac": "ctrl+cmd+left",
        "command": "git-more.stageSelector",
        "key": "ctrl+shift+meta+left"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "simple-git": "^3.19.0"
  }
}
